	WaveSpawnEvent event
	
	event.spawnAmount = 0
	event.eventFunction = BlockFurtherTitanfalls
	event.titanBlocked = shouldBlock
	event.waitTime = waitTime
	event.waveSubGroupName = subGroupName
	waveName.append(event)
}

